{
  "directoryListing": false,
  "headers": [
    {
      "source": "{**/*.html}",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "{**/*.ico,**/*.js,**/*.css}",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "{**/*.woff,**/*.woff2,**/*.ttf,**/*.eot}",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "{**/*.jpg,**/*.png,**/*.gif}",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/:authorOrTag?/:name?/rss",
      "destination": "/:authorOrTag?/:name?/rss.xml"
    }
  ],
  "redirects": [
    {
      "source": "/:slug/amp",
      "destination": "/:slug",
      "type": 302
    }
  ]
}
