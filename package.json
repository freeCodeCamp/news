{
  "name": "eleventy-news",
  "version": "1.0.0",
  "description": "Eleventy for https://www.freecodecamp.org/news",
  "main": ".eleventy.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env ELEVENTY_ENV=dev NODE_OPTIONS=--max-old-space-size=8192 eleventy --serve",
    "build": "cross-env ELEVENTY_ENV=prod NODE_OPTIONS=--max-old-space-size=8192 eleventy",
    "test": "npm run build",
    "lint": "eslint . --ext .js --cache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/freeCodeCamp/news"
  },
  "keywords": [
    "eleventy",
    "ghost",
    "starter",
    "boilerplate",
    "static",
    "site",
    "jamstack"
  ],
  "author": {
    "email": "team@freecodecamp.org",
    "name": "freeCodeCamp.org",
    "url": "https://www.freecodecamp.org"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/freeCodeCamp/news/issues"
  },
  "contributors": [
    "https://github.com/freeCodeCamp/news/graphs/contributors"
  ],
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@tryghost/content-api": "^1.5.4",
    "@tryghost/helpers": "^1.1.43",
    "clean-css": "^5.1.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.4",
    "dotenv": "^10.0.0",
    "eslint-utils": "^3.0.0",
    "i18next": "^21.0.0",
    "i18next-fs-backend": "^1.1.1",
    "terser": "^5.7.0"
  },
  "devDependencies": {
    "amperize": "0.6.1",
    "eslint": "7.32.0",
    "eslint-plugin-ghost": "2.6.0",
    "jsdom": "17.0.0",
    "lodash": "4.17.21",
    "md5": "2.3.0",
    "node-fetch": "2.6.5",
    "probe-image-size": "7.2.1",
    "sanitize-html": "2.5.2",
    "xml2js": "0.4.23"
  }
}
